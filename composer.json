{
    "name": "updivision/matrix-php-sdk",
    "description": "Matrix SDK for PHP",
    "keywords": ["matrix", "laravel", "sdk"],
    "type": "package",
    "require": {
        "guzzlehttp/guzzle": "~7.0",
        "php": ">=5.4.0"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.0|~5.0"
    },
    "autoload": {
        "psr-4": {
            "Updivision\\Matrix\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Alin Ghitu",
            "email": "alin@updivision.com"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Updivision\\Matrix\\MatrixServiceProvider"
            ],
            "aliases": {
                "Matrix": "Updivision\\Matrix\\Facades\\Matrix"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
